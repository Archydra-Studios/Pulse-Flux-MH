{
  "multipart": [
    {
      "apply": {
        "model": "pulseflux:block/logistic/solenoid_merge"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "pulseflux:block/logistic/solenoid_face"
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "pulseflux:block/logistic/solenoid_face",
        "y": 90
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "pulseflux:block/logistic/solenoid_face",
        "y": 180
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "pulseflux:block/logistic/solenoid_face",
        "y": 270
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "pulseflux:block/logistic/solenoid_face",
        "x": -90
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "pulseflux:block/logistic/solenoid_face",
        "x": 90
      }
    }
  ]
}